          <mxGeometry height="85" width="200" x="-1080" y="-460" as="geometry" />
        </mxCell>
        <mxCell id="FIdvG4DXuz2LYwi1fpN5-9" parent="1" style="rounded=1;whiteSpace=wrap;html=1;align=left;spacingLeft=10;spacingRight=10;" value="It is worthnoting that&amp;nbsp;&lt;b&gt;mchunk_prev_size&amp;nbsp;&lt;/b&gt;is sometimes not counted in size for optimization reasons" vertex="1">
